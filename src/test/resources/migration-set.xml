            <kindOpRestrictions type="simple" value="">
                <kindOpRules>
                    <!-- op = eq, lt, lte, gt, gte, regexp -->
                    <!-- property = name, value, key -->
                    <kindOpRule op="" property="" value=""/>
                </kindOpRules>
            </kindOpRestrictions>
        </kindOp>
    </kindOps>
</migrationset>